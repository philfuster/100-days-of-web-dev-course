<%- include('../shared/includes/head.ejs', { title: 'Welcome' }) %>
<link rel="stylesheet" href="/styles/product-list.css">
</head>
<body>
  <%-include('../shared/includes/header.ejs') %>
  <main>
    <h1>All Products</h1>
    <section id="product-list">
      <ul>
        <% for (const product of products) { %>
          <li>
            <%-include('../shared/includes/product-list-item.ejs', {product: product}) %>
          </li>
        <% } %>
      </ul>
    </section>
  </main>
</body>
</html>
